<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.minipig.mapper.SigninUserMapper">

    <insert id="save" parameterType="com.minipig.pojo.SigninUser">
        insert into student(name,salary) values(#{name},#{salary})
    </insert>

    <select id="findAll" resultType="com.minipig.pojo.SigninUser">
        select *
        from student
    </select>

</mapper>